<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <!-- This is temporary until these aspire packages are published to nuget.orgAspnetVersion -->
    <add key="aspire92" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/darc-pub-dotnet-aspire-0fcb1e98/nuget/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <!-- key value for <packageSource> should match key values from <packageSources> element -->
    <clear />
    <packageSource key="nuget">
      <package pattern="*" />
      <package pattern="Aspire.*" />
      <package pattern="Microsoft.Extensions.ServiceDiscovery*" />
    </packageSource>
    <packageSource key="aspire92">
      <package pattern="Aspire.*" />
      <package pattern="Microsoft.Extensions.ServiceDiscovery*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
